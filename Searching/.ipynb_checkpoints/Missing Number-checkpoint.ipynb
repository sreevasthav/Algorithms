   "source": [
    "# Finding the Missing Number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def FindMissNumber(A):\n",
    "    N = len(A)\n",
    "    x = 0\n",
    "    y = 0\n",
    "    for i in xrange(N):\n",
    "        x = x ^ A[i]\n",
    "    for j in xrange(1,N+2):\n",
    "        y = y ^ j\n",
    "    return x ^ y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {
    "collapsed": false
   },
