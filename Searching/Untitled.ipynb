   "source": [
    "# Repeated odd number of Times"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def repeatedOdd(A):\n",
    "    n = len(A)\n",
    "    x = 0\n",
    "    for i in xrange(n):\n",
    "        x = x ^ A[i]\n",
    "    return x\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
