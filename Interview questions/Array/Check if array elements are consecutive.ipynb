    "def Array_consecutive (A):\n",
    "    n = len(A)\n",
    "    a = min(A)\n",
    "    sum_th = n*(2*a + (n-1))/2\n",
    "    sum_p = 0\n",
    "    for i in A:\n",
    "        sum_p = sum_p + i\n",
    "    return sum_p == sum_th\n",
    "    \n",
    "    "
   ]
