    "def Missing(A):\n",
    "    n = len(A)\n",
    "    th_sum = (n+1)*(n+2)/2\n",
    "    pr_sum = 0\n",
    "    for i in xrange(n):\n",
    "        pr_sum = pr_sum + A[i]\n",
    "    result = th_sum - pr_sum\n",
    "    return result"
   ]
