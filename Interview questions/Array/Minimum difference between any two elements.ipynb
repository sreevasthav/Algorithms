   "source": [
    "def Min_diff (A):\n",
    "    n = len(A)\n",
    "    A.sort()\n",
    "    diff = 10**10\n",
    "    for i in xrange(1,n):\n",
    "        if abs(A[i]-A[i-1]) < diff:\n",
    "            diff = A[i] - A[i-1]\n",
    "        i = i + 1\n",
    "    return diff\n",
    "        "
   ]
