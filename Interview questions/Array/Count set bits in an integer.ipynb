   "source": [
    "# Count set bits in an integer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def Count_setBits(n):\n",
    "    count = 0\n",
    "    while n :\n",
    "        n = n & (n-1)\n",
    "        count = count + 1 \n",
    "    return count"
   ]
  },
